@model IEnumerable<Commissions>

<div id="padding-bot"class="display-4 text-center info">
    @Model.Count() commission(s) found:
</div>
<div class="HomeArtMuseum">

    @foreach (Commissions commission in Model)
    {
        <div class="col-sm-12 col-md-6 col-lg-4 text-center mb-3">
            <partial name="ComissionPartial.cshtml" model="commission"/>
        </div>
    }

</div>
